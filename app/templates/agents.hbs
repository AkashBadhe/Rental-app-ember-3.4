<div class="container agents-page">
  <div class="row">
    {{#each model as |agent|}}
      <div class="col-md-4 mb-5 agents">
        <div class="person-29381">
          <div class="media-39912">
            <img src={{agent.image}} alt="Image" class="img-fluid">
          </div>
          <h3>
            <a href="#">
              {{agent.name}}
            </a>
          </h3>
          <span class="meta d-block mb-4">
            {{agent.properties}}
          </span>
          <div class="social-32913">
            <a href="#">
              <span class="icon-facebook"></span>
            </a>
            <a href="#">
              <span class="icon-twitter"></span>
            </a>
            <a href="#">
              <span class="icon-instagram"></span>
            </a>
          </div>
        </div>
      </div>
    {{/each}}
  </div>
</div>